    "code": "PSYL11106",
    "name": "Applied Psychometrics",
    "url": "http://www.drps.ed.ac.uk/24-25/dpt/cxpsyl11106.htm",
    "availability": "SS1",
    "period": "Semester 2",
    "credits": "20",
