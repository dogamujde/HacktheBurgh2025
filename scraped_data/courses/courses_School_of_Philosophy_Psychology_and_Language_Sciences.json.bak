    "code": "PSYL10152",
    "name": "Perception, Action, Cognition",
    "url": "http://www.drps.ed.ac.uk/24-25/dpt/cxpsyl10152.htm",
    "availability": "SV1",
    "period": "Semester 2",
    "credits": "20",
    "subject": "Psychology",
    "school_name": "School of Philosophy, Psychology and Language Sciences",
