  {
    "code": "PHIL11233",
    "name": "Philosophy of Virtual Worlds MSc",
    "url": "http://www.drps.ed.ac.uk/24-25/dpt/cxphil11233.htm",
    "availability": "",
    "period": "Not delivered this year",
    "credits": "20",
    "subject": "Philosophy",
    "school_name": "School of Philosophy, Psychology and Language Sciences",
