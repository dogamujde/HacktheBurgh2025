  {
    "code": "PSYL10162",
    "name": "Identities and Collective Behaviour",
    "url": "http://www.drps.ed.ac.uk/24-25/dpt/cxpsyl10162.htm",
    "availability": "",
    "period": "Not delivered this year",
    "credits": "20",
    "subject": "Psychology",
    "school_name": "School of Philosophy, Psychology and Language Sciences",
