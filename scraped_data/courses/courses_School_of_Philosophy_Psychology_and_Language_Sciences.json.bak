  {
    "code": "LASC11183",
    "name": "Semiotics MSc",
    "url": "http://www.drps.ed.ac.uk/24-25/dpt/cxlasc11183.htm",
    "availability": "SV1",
    "period": "Semester 2",
    "credits": "20",
    "subject": "Language Sciences",
    "school_name": "School of Philosophy, Psychology and Language Sciences",
