  {
    "code": "LASC10111",
    "name": "Guided Research in Linguistics and English Language A",
    "url": "http://www.drps.ed.ac.uk/24-25/dpt/cxlasc10111.htm",
    "availability": "SV1",
    "period": "Semester 1",
