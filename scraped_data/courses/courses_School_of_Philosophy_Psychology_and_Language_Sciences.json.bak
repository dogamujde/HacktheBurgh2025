  {
    "code": "PHIL10183",
    "name": "Objectification, Dehumanisation, and Othering",
    "url": "http://www.drps.ed.ac.uk/24-25/dpt/cxphil10183.htm",
    "availability": "",
    "period": "Not delivered this year",
    "credits": "20",
