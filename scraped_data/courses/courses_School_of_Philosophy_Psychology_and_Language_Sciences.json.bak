  {
    "code": "PHIL11249",
    "name": "The Nature of Moral Understanding MSc",
    "url": "http://www.drps.ed.ac.uk/24-25/dpt/cxphil11249.htm",
    "availability": "SV1",
    "period": "Semester 1",
