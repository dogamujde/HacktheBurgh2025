    "code": "ECLG10009",
    "name": "Conservation and Management of Natural Populations",
    "url": "http://www.drps.ed.ac.uk/24-25/dpt/cxeclg10009.htm",
    "availability": "SS1",
    "period": "Block 3 (Sem 2)",
    "credits": "10",
