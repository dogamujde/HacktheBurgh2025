  {
    "code": "BICH11009",
    "name": "Practical Skills in Biochemistry A",
    "url": "http://www.drps.ed.ac.uk/24-25/dpt/cxbich11009.htm",
    "availability": "SS1",
    "period": "Semester 1",
    "credits": "20",
