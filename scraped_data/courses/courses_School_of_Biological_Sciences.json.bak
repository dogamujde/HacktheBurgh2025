  {
    "code": "MOGE10004",
    "name": "DNA Repair and Genome Instability",
    "url": "http://www.drps.ed.ac.uk/24-25/dpt/cxmoge10004.htm",
    "availability": "SS1",
    "period": "Semester 2",
    "credits": "10",
