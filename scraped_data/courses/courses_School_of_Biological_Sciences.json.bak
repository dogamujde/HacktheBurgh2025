  {
    "code": "IMMU10013",
    "name": "Th2 in Immunity, Allergy and Health",
    "url": "http://www.drps.ed.ac.uk/24-25/dpt/cximmu10013.htm",
    "availability": "SS1",
    "period": "Semester 1",
    "credits": "10",
