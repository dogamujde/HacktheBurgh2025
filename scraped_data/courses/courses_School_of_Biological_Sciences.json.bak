  {
    "code": "BILG08018",
    "name": "The Microbial World 2",
    "url": "http://www.drps.ed.ac.uk/24-25/dpt/cxbilg08018.htm",
    "availability": "SV1",
    "period": "Semester 2",
    "credits": "20",
