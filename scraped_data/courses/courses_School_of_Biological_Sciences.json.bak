    "code": "MSBM09001",
    "name": "Clinical Biochemistry and Endocrinology 3",
    "url": "http://www.drps.ed.ac.uk/24-25/dpt/cxmsbm09001.htm",
    "availability": "SV1",
    "period": "Semester 1",
    "credits": "20",
    "subject": "Biology",
