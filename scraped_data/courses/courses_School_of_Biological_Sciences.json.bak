  {
    "code": "BILG11004",
    "name": "Next Generation Genomics",
    "url": "http://www.drps.ed.ac.uk/24-25/dpt/cxbilg11004.htm",
    "availability": "SS1",
    "period": "Semester 2",
    "credits": "10",
    "subject": "Postgraduate",
