    "code": "BILG09015",
    "name": "Structures and Functions of Proteins 3",
    "url": "http://www.drps.ed.ac.uk/24-25/dpt/cxbilg09015.htm",
    "availability": "SV1",
    "period": "Semester 1",
    "credits": "20",
