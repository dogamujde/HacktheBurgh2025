  {
    "code": "BILG09023",
    "name": "Infection and Immunity 3",
    "url": "http://www.drps.ed.ac.uk/24-25/dpt/cxbilg09023.htm",
    "availability": "",
    "period": "Not delivered this year",
    "credits": "20",
    "subject": "Biology",
