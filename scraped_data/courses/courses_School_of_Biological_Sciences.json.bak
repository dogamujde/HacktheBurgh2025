  {
    "code": "BILG09011",
    "name": "Evolution and Ecology of Plants 3",
    "url": "http://www.drps.ed.ac.uk/24-25/dpt/cxbilg09011.htm",
    "availability": "SV1",
    "period": "Semester 2",
    "credits": "20",
