  {
    "code": "LAWS10286",
    "name": "Economic, Social and Cultural Rights",
    "url": "http://www.drps.ed.ac.uk/24-25/dpt/cxlaws10286.htm",
    "availability": "SV1",
    "period": "Semester 1",
    "credits": "20",
    "subject": "Law",
