  {
    "code": "LAWS10246",
    "name": "Critical issues in contemporary policing",
    "url": "http://www.drps.ed.ac.uk/24-25/dpt/cxlaws10246.htm",
    "availability": "",
    "period": "Not delivered this year",
    "credits": "20",
    "subject": "Law",
