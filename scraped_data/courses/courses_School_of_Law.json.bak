    "code": "LAWS10162",
    "name": "The Law of Fiduciary Duties",
    "url": "http://www.drps.ed.ac.uk/24-25/dpt/cxlaws10162.htm",
    "availability": "",
    "period": "Not delivered this year",
    "credits": "20",
