  {
    "code": "LAWS10238",
    "name": "Death and the Law",
    "url": "http://www.drps.ed.ac.uk/24-25/dpt/cxlaws10238.htm",
    "availability": "",
    "period": "Not delivered this year",
    "credits": "20",
