  {
    "code": "LAWS11188",
    "name": "Data Protection and Information Privacy",
    "url": "http://www.drps.ed.ac.uk/24-25/dpt/cxlaws11188.htm",
    "availability": "",
    "period": "Not delivered this year",
    "credits": "20",
