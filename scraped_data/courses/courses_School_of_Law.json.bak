  {
    "code": "LAWS11399",
    "name": "Risk and Regulation: Theories and Practices",
    "url": "http://www.drps.ed.ac.uk/24-25/dpt/cxlaws11399.htm",
    "availability": "",
    "period": "Not delivered this year",
    "credits": "20",
