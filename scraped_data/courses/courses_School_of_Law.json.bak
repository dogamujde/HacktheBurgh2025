    "code": "LAWS11508",
    "name": "Transnational waste law and circular economy",
    "url": "http://www.drps.ed.ac.uk/24-25/dpt/cxlaws11508.htm",
    "availability": "",
    "period": "Not delivered this year",
    "credits": "20",
