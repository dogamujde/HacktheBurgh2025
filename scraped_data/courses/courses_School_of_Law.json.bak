    "code": "LAWS08137",
    "name": "Introduction to Criminology",
    "url": "http://www.drps.ed.ac.uk/24-25/dpt/cxlaws08137.htm",
    "availability": "",
    "period": "Not delivered this year",
    "credits": "20",
