  {
    "code": "LAWS11448",
    "name": "Legal and Ethical Issues in Digital Journalism",
    "url": "http://www.drps.ed.ac.uk/24-25/dpt/cxlaws11448.htm",
    "availability": "",
    "period": "Not delivered this year",
    "credits": "10",
