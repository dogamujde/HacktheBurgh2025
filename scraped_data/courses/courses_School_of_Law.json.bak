    "code": "LAWS10225",
    "name": "Scots Law of Evidence: Principles, Policy and Practice",
    "url": "http://www.drps.ed.ac.uk/24-25/dpt/cxlaws10225.htm",
    "availability": "",
    "period": "Not delivered this year",
    "credits": "20",
