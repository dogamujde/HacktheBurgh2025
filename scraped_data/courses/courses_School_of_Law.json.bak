    "code": "LAWS10282",
    "name": "End of Life Ethics and Law",
    "url": "http://www.drps.ed.ac.uk/24-25/dpt/cxlaws10282.htm",
    "availability": "SV1",
    "period": "Semester 1",
    "credits": "20",
