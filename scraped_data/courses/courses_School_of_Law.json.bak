  {
    "code": "LAWS11463",
    "name": "Legal Technology and the Justice System",
    "url": "http://www.drps.ed.ac.uk/24-25/dpt/cxlaws11463.htm",
    "availability": "SS1",
    "period": "Semester 2",
    "credits": "20",
