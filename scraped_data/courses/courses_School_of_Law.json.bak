    "code": "LAWS08136",
    "name": "Introduction to Criminal Justice",
    "url": "http://www.drps.ed.ac.uk/24-25/dpt/cxlaws08136.htm",
    "availability": "SV1",
    "period": "Semester 1",
    "credits": "20",
