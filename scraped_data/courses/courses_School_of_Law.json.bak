  {
    "code": "LAWS11221",
    "name": "Inter-state Conflict and Humanitarian Law",
    "url": "http://www.drps.ed.ac.uk/24-25/dpt/cxlaws11221.htm",
    "availability": "SS1",
    "period": "Semester 2",
    "credits": "20",
    "subject": "Law",
