    "code": "LAWS10270",
    "name": "Competition Law and Economics",
    "url": "http://www.drps.ed.ac.uk/24-25/dpt/cxlaws10270.htm",
    "availability": "SV1",
    "period": "Semester 1",
    "credits": "20",
