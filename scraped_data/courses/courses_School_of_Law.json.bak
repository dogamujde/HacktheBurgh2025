  {
    "code": "LAWS11125",
    "name": "Intellectual Property Law 1: Copyright and Related Rights",
    "url": "http://www.drps.ed.ac.uk/24-25/dpt/cxlaws11125.htm",
    "availability": "SV1",
    "period": "Semester 1",
    "credits": "20",
    "subject": "Law",
