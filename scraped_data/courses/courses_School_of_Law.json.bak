  {
    "code": "LAWS10247",
    "name": "Gifts in Context",
    "url": "http://www.drps.ed.ac.uk/24-25/dpt/cxlaws10247.htm",
    "availability": "SV1",
    "period": "Semester 1",
    "credits": "20",
